<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <!-- Plugin Informations -->
    <name>socialhub-rating</name>
    <class>fr.paris.lutece.plugins.socialhub.modules.rating.service.RatingPlugin</class>
    <version>1.0.0</version>
    <description>module.module.socialhub.rating.module.description</description>
    <provider>module.socialhub.rating.module.provider</provider>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/socialhub/modules/rating/socialhub-rating.png</icon-url>
    <copyright>Copyright 2001-2012 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>3.1.6</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->
    <db-pool-required>1</db-pool-required>
    
    <!--Administration interface parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>SOCIALHUB_VOTE_TYPES_MANAGEMENT</feature-id>
            <feature-title>module.socialhub.rating.adminFeature.vote_types_management.name</feature-title>
            <feature-description>module.socialhub.rating.adminFeature.vote_types_management.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/socialhub/modules/rating/ManageVoteTypes.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/socialhub/modules/rating/socialhub-rating.png</feature-icon-url>
            <feature-documentation-url>jsp/admin/documentation/AdminDocumentation.jsp?doc=admin-socialhub-rating</feature-documentation-url>
        </admin-feature>
    </admin-features>
    
    <!-- Specific plugin CSS stylesheet -->
    <css-stylesheets>
        <css-stylesheets-scope>portal</css-stylesheets-scope>
        <css-stylesheet>../../js/jquery/plugins/star-rating/jquery.rating.css</css-stylesheet>
    </css-stylesheets>
    <!-- Specific plugin JAVASCRIPT -->
    <javascript-files>
        <javascript-files-scope>portal</javascript-files-scope>
        <javascript-file>socialhub/modules/rating/socialhub-rating.js</javascript-file>
        <javascript-file>../jquery/plugins/star-rating/jquery.rating.js</javascript-file>
    </javascript-files>
    
</plug-in>